{
  "name": "gatsby-transformer-gitinfo",
  "description": "Gatsby transformer plugin that adds git info to File nodes created by gatsby-source-filesystem",
  "version": "1.4.0",
  "dependencies": {
    "@babel/runtime": "7.21.0",
    "simple-git": "3.16.1"
  },
  "devDependencies": {
    "@babel/cli": "7.21.0",
    "@babel/core": "7.20.12",
    "babel-preset-gatsby-package": "3.7.0",
    "cross-env": "7.0.3",
    "jest": "^29.7.0"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "git"
  ],
  "license": "MIT",
  "peerDependencies": {
    "gatsby": "^5.13.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/edunga1/gatsby-transformer-gitinfo.git"
  },
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__",
    "test": "jest"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
